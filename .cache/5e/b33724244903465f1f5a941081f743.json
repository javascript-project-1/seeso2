\");\n  ctx.clearRect(0, 0, canvas.width, canvas.height );\n}\n\nfunction showGaze(gazeInfo) {\n  showGazeInfoOnDom(gazeInfo)\n  showGazeDotOnDom(gazeInfo)\n}\n\nfunction hideGaze(){\n  hideGazeInfoOnDom();\n  hideGazeDotOnDom();\n}\n\nexport { showGaze, hideGaze }"},"lineCount":null}},"error":null,"hash":"d2f2c98ff5402c27635a8249c1bf8c10","cacheData":{"env":{}}}